record(ao, "$(P)Temperature")
{
    field(DESC, "Temperature")
    field(DTYP, "asynInt32")
    field(OUT,  "@asyn($(PORT),$(ADDR),$(TIMEOUT=1))CPSC_TEMPERATURE")
    field(VAL,  293)
    field(PINI, "YES")
}

record(ao, "$(P)FrequencyX")
{
    field(DESC, "X axis frequency")
    field(DTYP, "asynInt32")
    field(OUT,  "@asyn($(PORT),$(ADDR),$(TIMEOUT=1))CPSC_FREQUENCY_X")
    field(VAL,  600)
    field(PINI, "YES")
}

